{
  "compilerOptions": {
    "outDir": "./lib",
    "baseUrl": "./src",
    "target": "es6",
    "module": "commonjs",
    "lib": ["es6"],
    "declaration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
